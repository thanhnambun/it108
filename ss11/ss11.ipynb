   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(0, 10, 50)\n",
    "\n",
    "y = x**2 + 2*x\n",
    "\n",
    "plt.plot(x, y, label=\"y = x^2 + 2x\")\n",
    "\n",
    "plt.xlabel(\"Trục x\")\n",
    "plt.ylabel(\"Trục y\")\n",
    "\n",
    "plt.grid(True, linestyle=\"--\")\n",
    "\n",
    "plt.legend()\n",
    "plt.show()\n"
   ]
  }
